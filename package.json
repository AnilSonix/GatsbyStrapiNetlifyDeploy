    "framer-motion": "^4.1.3",
    "gatsby": "^3.2.1",
    "gatsby-plugin-image": "^1.2.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-source-strapi": "0.0.12",
    "gatsby-transformer-sharp": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-gallery-carousel": "^0.1.0"
  },
  "devDependencies": {
    "prettier": "2.2.1"
